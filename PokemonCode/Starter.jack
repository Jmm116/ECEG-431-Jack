class Starter {
    field boolean exit;
    field Cycle cycle;
    field Array starters;
    field str starter;

    constructor Starter new() {
        let cycle = Cycle.new(starters);
        let exit = false;
        let starters[0] = Venusaur;
        let starters[1] = Blastoise;
        let starters[2] = Charizard;
    }

    method void selector() {
        let pokemon = cycle.cycleThrough();
    }
}