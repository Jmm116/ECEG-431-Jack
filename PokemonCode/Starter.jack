class Starter {
    field boolean exit;
    field Array starters;
    field String starter; 

    constructor Starter new() {
        let exit = false;
        let starters[0] = "Venusaur";
        let starters[1] = "Blastoise";
        let starters[2] = "Charizard";

        let starter = Cycle.cycleThrough(starters, 3);
        return this;
    }

    method String getStarter() {
        return starter;
    }
}
