class Starter {
    field boolean exit;
    field Cycle cycle;
    field Array starters;
    field String starter; 

    constructor Starter new() {
        let exit = false;
        let starters[0] = "Venusaur";
        let starters[1] = "Blastoise";
        let starters[2] = "Charizard";
        let cycle = Cycle.new(starters, 3);

        let starter = do cycle.cycleThrough();
    }

    function String getStarter() {
        return starter;
    }
}